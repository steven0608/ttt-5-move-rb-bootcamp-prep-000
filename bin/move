#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here

puts "Welcome to Tic Tac Toe!"
puts "Where would you like to go?"
input=gets.strip
def input_to_index(input)
  input=input.to_i
  index= input - 1
  return index
end
input_to_index(input)
board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
move(board,index,"X")
board
display_board(board)
